	xShake2 = -53;
	//TweenLite.to($box2, 2, {y:1000});
	//count2 = 4;
}

function mouseOut4() {
  speedVar = 2.5;
  xShake1 = -50;
  xShake2 = -50;
  setTimeoutDelay = 2000;
  
}
		
